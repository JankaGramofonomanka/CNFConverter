entrypoints Formula ;
Equ.   Formula1 ::= Formula2 "<=>" Formula2 ;
RImpl. Formula2 ::= Formula3 "==>" Formula3 ;
LImpl. Formula2 ::= Formula3 "<==" Formula3 ;
And.   Formula3 ::= Formula4 "and" Formula4 ;
Or.    Formula4 ::= Formula5 "or" Formula5 ;
XOr.   Formula4 ::= Formula5 "xor" Formula5 ;
Not.   Formula5 ::= "not" Formula6 ;
Var.   Formula6 ::= Ident ;

coercions Formula 6 ;

comment    "#" ;
